    "#엑셀파일 합치기\n",
    "\n",
    "import os\n",
    "import pandas as pd\n",
    "cwd = os.path.abspath('') \n",
    "files = os.listdir(cwd)  \n",
    "print(files)\n",
    "\n",
    "df = pd.DataFrame()\n",
    "for file in files:\n",
    "    if file.endswith('.xlsx'):\n",
    "        df = df.append(pd.read_excel(file), ignore_index=true) \n",
    "df.head() \n",
    "df.to_excel('total_sales.xlsx')"
   ]
  }
